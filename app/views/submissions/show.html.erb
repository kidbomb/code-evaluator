<p id="notice"><%= notice %></p>

<h2> Submission #<%=@submission.id %></h2>

<p><b>Test case results</b></p>
<table class="table">
  <tr>
    <td>Test id</td>
    <td>Result</td>
  </tr>
<% @submission.test_case_results.each do |test_case_result| %>
  <tr>
     <td><%= test_case_result.test_case_id %></td>
     <td><span class="label label-<%=format_status_bootstrap(test_case_result.status)%>"> <%= format_status(test_case_result.status) %></span> </td>
  </tr>
<% end %>
</table>
<%= button_to 'Back to problem', problem_path(@problem), :method=> "get", :class=>"btn btn-default" %>
