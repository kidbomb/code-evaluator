<h1>Listing submissions</h1>

<table class="table">
    <tr>
      <th>Submission #</th>
      <th></th>
      <th></th>
    </tr>

    <% @submissions.each do |submission| %>
      <tr>
        <td><%= submission.id %></td>
        <td><%= button_to 'Show', submission, :method=>"get", :class=>"btn btn-default" %></td>
        <td></td>
      </tr>
    <% end %>
</table>

<br>

<%= button_to 'New submission', new_problem_submission_path,:method=>"get", :class=>"btn btn-default" %>
